<?xml version="1.0" encoding="utf-8" ?>
<MasterDetailPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                  xmlns:pages="clr-namespace:EuropeAesth.MasDetPage"
                  xmlns:europeaesth="clr-namespace:EuropeAesth"
                  x:Class="EuropeAesth.MasDetPage.MainPage"
                  NavigationPage.HasNavigationBar="False" >
  <MasterDetailPage.Master>
    <pages:MainPageMaster x:Name="MasterPage" />
  </MasterDetailPage.Master>
  <MasterDetailPage.Detail>
        <NavigationPage x:Name="naviPage" BarTextColor="#62aa93" IconImageSource="ic_adjuvan_actionBar.png">
      <x:Arguments>
        <europeaesth:TabbedMainPage />
      </x:Arguments>
    </NavigationPage>
  </MasterDetailPage.Detail>
</MasterDetailPage>