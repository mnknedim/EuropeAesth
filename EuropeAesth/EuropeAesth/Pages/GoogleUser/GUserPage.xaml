<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="EuropeAesth.Pages.GoogleUser.GUserPage">
    <ContentPage.ToolbarItems>
        <ToolbarItem IconImageSource="ic_exit.png" Command="{Binding GoogleLogoutCommand}" x:Name="CikisButon" />
    </ContentPage.ToolbarItems>
    <ContentPage.Content>
        <StackLayout>
            <Label Text="{Binding GProfil.Name}"/>
            <Label Text="{Binding GProfil.GivenName}"/>
            <Label Text="{Binding GProfil.Email}"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>